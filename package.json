{
  "name": "@fika/cli",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.3.0",
  "description": "Terminal CLI for enjoying my spare time.",
  "author": "ShenQingchuan <shen_qing_chuan@163.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://git.heteronova.cc/shenqingchuan/Fika.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "bin": {
    "fika": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "run": "esno src/index.ts",
    "build": "unbuild",
    "watch": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "debug": "^4.3.5",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.1.2",
    "ora": "^8.0.1",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.0",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.8",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.14.2",
    "bumpp": "^9.4.1",
    "eslint": "^9.4.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.5",
    "pnpm": "^9.3.0",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.13",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
